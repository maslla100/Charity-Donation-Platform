{
  "name": "charity-donation-platform",
  "version": "1.0.0",
  "description": "Charity Org",
  "main": "server/server.js",
  "scripts": {
    "start": "concurrently \"npm run start --prefix server\" \"npm run start --prefix client\"",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && client && npm run dev",
    "install": "cd server && npm install && cd .. && cd client && npm install",
    "build": "cd client && npm run build",
    "seed": "cd server && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maslla100/Charity-Donation-Platform.git"
  },
  "author": "Luis Llamas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maslla100/Charity-Donation-Platform/issues"
  },
  "homepage": "https://github.com/maslla100/Charity-Donation-Platform#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0"
  }
}